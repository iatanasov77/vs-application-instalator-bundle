{
	"name" : "vankosoft/users-bundle",
	"type" : "symfony-bundle",
	"description" : "Symfony VSUsersBundle",
	"keywords" : [
		"User management"
	],
	"homepage" : "https://github.com/iatanasov77/vs-users-bundle",
	"license" : "MIT",
	"authors" : [{
			"name" : "Ivan I. Atanasov",
			"email" : "i.atanasov77@gmail.com"
		}
	],
	"require" : {
		"php" : "^8.1",
		"vankosoft/application-bundle" : "^1.10",
		"symfony/security-bundle" : "^6.4",
		"symfony/mailer" : "^6.4",
		"symfonycasts/verify-email-bundle" : "^1.16",
		"symfonycasts/reset-password-bundle" : "1.20.*",
		"symfony/ux-toggle-password" : "^2.15"
	},
	"conflict" : {
		"doctrine/doctrine-bundle" : "<1.3",
		"symfony/doctrine-bridge" : "<2.7"
	},
	"autoload" : {
		"psr-4" : {
			"Vankosoft\\UsersBundle\\" : "lib/"
		}
	},
	"repositories" : [{
			"type" : "path",
			"url" : "../*"
		}
	],
	"minimum-stability" : "dev"
}