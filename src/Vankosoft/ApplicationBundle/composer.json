{
	"name" : "vankosoft/application-bundle",
	"type" : "symfony-bundle",
	"description" : "Symfony VSApplicationBundle",
	"keywords" : [
		"Symfony Application"
	],
	"homepage" : "https://github.com/iatanasov77/vs-application-bundle",
	"license" : "MIT",
	"authors" : [{
			"name" : "Ivan I. Atanasov",
			"email" : "i.atanasov77@gmail.com"
		}
	],
	"require" : {
		"php" : "^8.1",
		"symfony/monolog-bundle" : "^3.8",
		"symfony/webpack-encore-bundle": "^1.16",
		"symfony/templating": "^5.4 || ^6.1",
		"twig/extra-bundle" : "^3.3",
		"twig/string-extra": "^3.3",
		"twig/intl-extra": "^3.5",
		"doctrine/doctrine-migrations-bundle": "^3.2",
		"stof/doctrine-extensions-bundle": "^1.7",
		"knplabs/knp-menu-bundle" : "^3.1",
		"knplabs/knp-paginator-bundle": "^5.9",
		"knplabs/knp-time-bundle": "^2.1",
		"sylius-labs/doctrine-migrations-extra-bundle": "^0.2",
		"sylius/resource-bundle": "^1.10",
		"sylius/taxonomy": "^1.11",
        "sylius/locale": "^1.11",
		"sylius/theme-bundle" : "^2.2",
		"friendsofsymfony/jsrouting-bundle": "^3.2",
        "willdurand/js-translation-bundle": "^5.0",
		"hackzilla/password-generator-bundle": "^5.0"
	},
	"suggest" : {
		"symfony/debug-bundle" : "Allows developement debugging",
		"symfony/maker-bundle" : "Standard symfony console maker tools",
		"symplify/changelog-linker" : "Allows generation of nice CHANGELOG.md file"
	},
	"conflict" : {
		"doctrine/doctrine-bundle" : "<1.3",
		"symfony/doctrine-bridge" : "<2.7"
	},
	"autoload" : {
		"psr-4" : {
			"Vankosoft\\ApplicationBundle\\" : "/"
		}
	},
	"repositories" : [{
			"type" : "path",
			"url" : "../*"
		}
	],
	"minimum-stability" : "dev"
}